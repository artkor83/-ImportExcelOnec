			Сообщить("Файл алгоритма "+ТекФайл.Имя+" не обнаружен")
		КонецЕсли;
	
	КонецЦикла;
	
	Сообщить("Загружено "+КолАлгоритмов + " алгоритмов");
	
	ОбновитьКолАлгоритмов();
	
